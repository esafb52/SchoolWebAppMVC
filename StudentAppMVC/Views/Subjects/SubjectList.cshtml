@model StudentAppMVC.View_Models.SubjectListViewModel

@{
    Layout = null;
}

<div class="form-group">
    @Html.LabelFor(model => model.SubjectId, htmlAttributes: new { @class = "control-label col-md-2" })
    <div class="col-md-10">
        @Html.DropDownListFor(model => model.SubjectId, Model.Subjects, htmlAttributes: new { @class = "form-control" })
        @Html.ValidationMessageFor(model => model.SubjectId, "", new { @class = "text-danger" })
    </div>
</div>
